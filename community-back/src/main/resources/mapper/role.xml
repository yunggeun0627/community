<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.communityback.domain.role.RoleMapper">
    <select id="findByRoleName" resultType="com.korit.communityback.domain.role.Role">
        select
            *
        from
            role_tb
        where
            role_name = #{roleName}
    </select>
</mapper>