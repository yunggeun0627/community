<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.communityback.domain.userRole.UserRoleMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="userRoleId">
        insert into user_role_tb
        values (default, #{userId}, #{roleId})
    </insert>
    <delete id="deleteByUserId">
        delete
        from
        user_role_tb
        where
        user_id = #{user_Id}
    </delete>
    <delete id="deleteByUserIds">
        delete
        form
        user_role_tb
        where
        user_id in <foreach item="userId" collection="userIds" open="(" close=")"  separator=","> #{userId} </foreach>
    </delete>
</mapper>